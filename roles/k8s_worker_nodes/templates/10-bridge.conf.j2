{
  "cniVersion": "1.1.0",
  "name": "bridge",
  "type": "bridge",
  "bridge": "cni0",
  "isGateway": true,
  "ipMasq": true,
  "ipam": {
    "type": "host-local",
    "ranges": [
      [{
        "subnet": "{{ item.value.pod_subnet }}"
      }]
    ],
    "routes": [{
      "dst": "0.0.0.0/0"
    }]
  }
}